{
  "name": "@shrub/subgraph-paper-gardens",
  "version": "0.1.0",
  "scripts": {
    "auth": "npx graph auth --product hosted-service https://api.thegraph.com/deploy/",
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create jguthrie7/shrub-paper-gardens --node https://api.thegraph.com/deploy/",
    "create-local": "graph create jguthrie7/shrub-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy jguthrie7/shrubpapergardens --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy jguthrie7/shrub-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "remove-local": "graph remove --node http://127.0.0.1:8020 jguthrie7/shrub-subgraph",
    "deploy-again-local": "yarn codegen; yarn remove-local; yarn create-local; yarn deploy-local"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0"
  },
  "dependencies": {
    "@protofire/subgraph-toolkit": "0.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
